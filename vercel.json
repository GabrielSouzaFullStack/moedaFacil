{
  "functions": {
    "api/*.php": {
      "runtime": "vercel-php@0.6.0"
    },
    "public/*.php": {
      "runtime": "vercel-php@0.6.0"
    }
  },
  "routes": [
    {
      "src": "/styles/(.*)",
      "dest": "/public/styles/$1"
    },
    {
      "src": "/img/(.*)",
      "dest": "/public/img/$1"
    },
    {
      "src": "/(.*\\.php)",
      "dest": "/public/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/public/index.php"
    }
  ],
  "builds": [
    {
      "src": "public/**/*.php",
      "use": "vercel-php@0.6.0"
    }
  ]
}